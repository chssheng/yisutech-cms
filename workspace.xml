<?xml version="1.0" encoding="UTF-8"?>
<workspace>
    <!-- packages used to filter classes, then container start faster! -->
    <deploy package="com.yisutech.erp.cms">
        <mapping src="dao-1.0-SNAPSHOT.jar" dest="dao/target/classes"/>
        <mapping src="tools-1.0-SNAPSHOT.jar" dest="tools/target/classes"/>
        <mapping src="biz-1.0.0-SNAPSHOT.jar" dest="biz/target/classes"/>
        <mapping src="framework-1.0.0-SNAPSHOT.jar" dest="framework/target/classes"/>
        <mapping src="webroot" dest="app/src/main/webapp"/>
        <mapping src="WEB-INF/classes" dest="app/target/classes"/>
        <mapping src="WEB-INF/pages" dest="app/src/main/webapp/WEB-INF/pages"/>
    </deploy>

    <plugins>
        <plugin name="spring_plugin"></plugin>
        <plugin name="ibatis_plugin"></plugin>
        <plugin name="webx3_plugin"></plugin>
        <plugin name="sofa_plugin"></plugin>
    </plugins>
</workspace>
